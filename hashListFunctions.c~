
int  addGene(int geneID){
  extern upstreamGene *pathway;
  upstreamGene *g;
  g = malloc(sizeof(upstreamGene));
  if(g==NULL)
	return -1;
  g->id = geneID;
  g->down = NULL;
  HASH_ADD_INT(pathway, id, g);
  return 0;
}


upstreamGene* findGene(int geneID){
  extern upstreamGene *pathway;
  upstreamGene *g;
  HASH_FIND_INT(pathway, &geneID, g);
  return g;
}

int addInteraction(int upID, int downID, relationType rel){
  extern upstreamGene *pathway;
  upstreamGene *g;
  downList     *d;
  g = findGene(upID);
  if(g == NULL)
	return -1;
  d = malloc(sizeof(downList));
  d->id = downID;
  d->relation = rel;
  d->next = g->down;
  g->down = d;
  return 0;
}
